{
  "name": "manrix/slim-db-backups",
  "description": "Simple application to manage database backups.",
  "keywords": [
    "php",
    "slim",
    "database",
    "backup"
  ],
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-pdo": "*",
    "aura/sql": "^3.0",
    "aura/sqlquery": "^2.7",
    "laminas/laminas-authentication": "^2.7",
    "laminas/laminas-session": "^2.9",
    "php-di/php-di": "^6.0",
    "selective/basepath": "^0.1.1",
    "slim/csrf": "^1.0",
    "slim/http": "^0.8.0",
    "slim/psr7": "^0.6.0",
    "slim/slim": "^4.3",
    "slim/twig-view": "3.0.0",
    "spatie/db-dumper": "^2.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
